package com.gagan.UseCases;

import com.gagan.Dao.EmployeeDaoImpl;
import com.gagan.Entity.Employee;

public class EmployeeUseCases {

	public static void main(String[] args) {
		EmployeeDaoImpl Dao = new EmployeeDaoImpl();
		Employee e = new Employee(3, "golu", "bala", 100);
		System.out.println(Dao.saveEmploye(e)) ;
	}
}
