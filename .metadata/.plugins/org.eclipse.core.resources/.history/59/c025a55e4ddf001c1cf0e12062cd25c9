package javaday4;

public class OddEven {
  
	void Odd(int n) {
		if(n<0) {
			System.out.println("Error");
		}
		
	   else if(n%2==0) {
			int a = n - 10 ;
			int ans = a+n ;
			System.out.println(ans);
		}
		else if(n%2!=0) {
			System.out.println(n);
		}
	}
	public static void main(String[] args) {
		OddEven n1 = new OddEven(); 
		n1.Odd(34);
		n1.Odd(67);
		n1.Odd(-567);
	}
}
