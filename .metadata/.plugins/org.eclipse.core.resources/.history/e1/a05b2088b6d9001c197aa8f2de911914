package assign3_1;

public class Vowel {
	String ch = "a" ;
//	String co = "f" ;
//	String er = "#" ;
	
	void check() {
		if(ch=="a" ||ch=="e" ||ch=="i" ||ch=="o" ||ch=="u") {
			System.out.println("Vowel");
		}
		else if(ch=="!" ||ch=="@" ||ch=="#" ||ch=="$" ||ch=="%") {
			System.out.println("invalid");
		}
		else {
			System.out.println("Consonant");
		}
		
	}
	
	
 public static void main(String[]args) {
	 Vowel a1 = new Vowel();
//	 System.out.println(a1.ch);
	 a1.check();
	 a1.ch="r" ;
	 a1.check();
	 a1.ch = "$" ;
	 a1.check();
 }
}
